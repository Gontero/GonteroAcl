{
    "name": "gontero/gontero-acl",
    "description": "Acl based on annotations",
    "type": "library",
    "keywords": [
        "annotation",
        "acl"
    ],
    "authors": [
        {
            "name": "Robert Ryba",
            "email": "robertry@poczta.onet.pl",
            "homepage": "https://github.com/Gontero"
        }
    ],
    "require": {
        "php": ">=5.5",
        "zendframework/zend-authentication":">=2.4",
        "zendframework/zend-code":">=2.4",
        "zendframework/zend-servicemanager": ">=2.4",
        "zendframework/zend-eventmanager": ">=2.4",
        "zendframework/zend-modulemanager": ">=2.4",
        "zendframework/zend-mvc": ">=2.4",
        "zendframework/zend-view": ">=2.4",
        "zendframework/zend-serializer": ">=2.4",
        "zendframework/zend-log": ">=2.4",
        "zendframework/zend-i18n": ">=2.4",
        "zendframework/zend-http": ">=2.4",
        "zendframework/zend-console": ">=2.4",
        "doctrine/common": ">=2.1",
        "doctrine/doctrine-orm-module": "0.7.*"
    },
    "autoload": {
        "psr-4": {
            "GonteroAcl\\": "src/",
            "GonteroAclTest\\": "tests/GonteroAcl"
        },
        "classmap": [
            "./Module.php"
        ]
    },
    "require-dev": {
        "phpunit/phpunit": "4.7.*",
        "zendframework/zend-loader": ">=2.4"
    }
}
